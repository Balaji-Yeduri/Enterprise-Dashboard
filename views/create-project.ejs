                <div class="mt-4 mb-4">
                    <h3 class=" text-themecolor mb-0>Dashboard">Create Project</h3>
                </div>
                <form>
                    <div class="row mb-4">
                        <div class="col">
                            <div class="form-outline">
                                    <fieldset>
                                            <legend>Project Details</legend>
                                <label class="form-label">Project Name</label>
                                <input type="text" id="project_name" class="form-control" required/>
                            </div>
                        </div>
                        <div class="col" style="padding-top: 2.7rem;">
                            <div class="form-outline">
                                <label class="form-label" >Alias</label>
                                <input type="text" id="alias" class="form-control" />
                            </div>
                        </div>
                    </div>
                    <div class="row mb-4">
                        <div class="col">
                            <div class="form-outline">
                                    <label class="text-muted mr-4" >Monitoring Type</label>
                                    <select class="form-control" id="monitoring_type" onchange="yesnoCheck(this);">
                                    <option value="select">Select</option>
                                    <option value="asknow">Asknow based</option>
                                    <option value="api">API based</option>
                                    </select>
                            </div>
                        </div>
                        <div class="col">
                            <div class="form-outline">
                                    <div id="frequency" style="display: none;">
                                            <label class="text-muted mr-3" >Monitoring Frequency</label>
                                            <input type="number" id="monitoring_frequency" class="form-control" />
                                        </div>
                            </div>
                        </div>
                    </div>
                    <div class="row mb-4">
                         <div class="col">
                            <div class="form-outline">
                                    <div id="sysid" style="display: none;">
                                            <label class="text-muted mr-3" >Sys Id</label>
                                            <input type="text" id="sys_id" class="form-control" />
                                        </div>
                            </div>
                        </div> 
                    </div>
                        <div class="row mb-4">
                        <div class="col" >
                            <div class="form-outline">
                                <label class="text-muted mr-4" >Dependencies Available</label>
                                <select class="form-control" id="dependencies_available" onchange="yesnoCheck(this);" required>
                                        <option value="select">Select</option>
                                    <option value="yes">Yes</option>
                                    <option value="no">No</option>
                                </select>
                            </div>
                        </div>
                        <div class="col">
                            <div class="form-outline">
                                    <label class="text-muted mr-3" > Resource Group Email Id</label>
                                    <input type="text" id="contact_email_id" class="form-control" />
                                <!-- </fieldset> -->
                            </div>
                        </div>
                    </fieldset>
                    </div>
                    <div id="dependency" style="display: none;">
                    <div class="row mb-4">
                        <div class="col">
                            <div class="form-outline">
                                
                                <fieldset>
                                    <legend>Dependency List</legend>
                                    <label class="text-muted mr-3" >Project Name</label>
                                       <select class="form-control " id="dependency_project_name" >
                                        <option value="1">One</option>
                                        <option value="2">Two</option>
                                        <option value="8">Eight</option>
                                      </select>
                                    </select>
                                </div>
                            </div>
                            <div class="col" style="padding-top: 2.7rem;">
                                <div class="form-outline">
                                    <label class="text-muted mr-3" >Required For Operation</label>
                                    <select class="form-control" id="required_for_operation">
                                        <option>Yes</option>
                                        <option>No</option>
                                    </select>
                                </fieldset>
                            </div>
                            </div>
                        </div>
                    </div>
                    <div class="row mb-4">
                        <div class="col" style="padding-right: 35.5rem;">
                            <div class="form-outline" >
                                    <fieldset>
                                    <legend>DownStream List</legend>
                                    <label class="text-muted mr-3" >Project Name</label>
                                    <select class="form-control" id="downstream_project_name">
                                        <option>Yes</option>
                                        <option>No</option>
                                    </select>
                                </fieldset>
                            </div>
                        </div>
                    </div>
                    <div class="row mb-4">
                        <div class="col" style="padding-left: 1.5rem;">
                            <div class="form-outline">
                                <fieldset>
                                    <legend>POC Details</legend>
                                    <label class="text-muted mr-3" >Senior POC Name</label>
                                    <input type="text" id="sr_poc_name" class="form-control" required/>
                                    <label class="text-muted mr-3" >Senior POC Email</label>
                                    <input type="text" id="sr_poc_mail" class="form-control" required/>
                                </fieldset>
                            </div>
                        </div>

                        <div class="col" style="padding-top: 3rem;">
                            <div class="form-outline">
                                    <label class="text-muted mr-3" >Secondary POC Name</label>
                                    <input type="text" id="scndry_poc_name" class="form-control" required/>
                                    <label class="text-muted mr-3" >Secondary POC Email</label>
                                    <input type="text" id="scndry_poc_mail" class="form-control" required/>
                                </fieldset>
                            </div>
                        </div>
                        </div>
                    <div class="row mb-4" >
                            <div class="col"style="padding-left: 1.5rem;">
                                    <div class="form-outline">
                        <fieldset>
                            <legend>SLA Criteria</legend>
                            <label class="text-muted mr-3" >P1</label>
                            <input type="number" id="p1" class="form-control" />
                        </fieldset>
                    </div>
                </div>
                        <div class="col" style="padding-top: 2.7rem;">
                            <div class="form-outline">
                                <label class="text-muted mr-3" >P2</label>
                                <input type="number" id="p2" class="form-control" />
                            </div>
                        </div>
                        </div>
                    </div>
                    <div class="row mb-4" style="padding-left: 2rem;" >
                        <div class = "col" >
                            <div class="form-outline">
                                <fieldset>
                                    <legend>Cost to Company</legend>
                            <label class="text-muted mr-3" >FTE Count</label>
                            <input type="number" id="fte_count" class="form-control" required/>
                        </div>
                    </div>
                        <div class="col" style="padding-top: 2.7rem;">
                            <div class="form-outline">
                                <label class="text-muted mr-3" >FTE Cost</label>
                                <input type="number" id="fte_cost" class="form-control" required/>
                                </fieldset>
                            </div>
                        </div>
                    </div>
                    <button type="submit" class="mb-3 btn btn-primary project-submit-button">Submit</button>

                    <button type="button" class="mb-3 btn btn-secondary project-submit-button">Cancel</button>
                </form>
            </div>
