          <li class="active">
            <a href="tickets.html">
              <i class="fa-solid fa-ticket icon"></i>
              <p>Tickets</p>
            </a>
          </li>
        </ul>
      </div>
    </div>
    <div class="container-fluid" style="height: 100vh;">
 
      <div class="content ml-4">
        <div class="mt-4 mb-4">
          <h3 class=" text-themecolor mb-0>Dashboard">Tickets</h3>
        </div>
        <div class=" mt-5">
          <div class="row">
            <div>
              <form>
                <div class="input-group no-border search-box">
                  <input type="text" value="" class="form-control" placeholder="Search...">
                  <div class="input-group-append">
                    <div class="input-group-text">
                      <i class="nc-icon nc-zoom-split"></i>
                    </div>
                  </div>
                </div>
              </form>
            </div>

            <diV style="margin-left: 620px;">
              <select class="form-control" id="order-sort">
                <option>All</option>
                <option>Critical</option>
                <option>Warning</option>
                <option>Good</option>
              </select>
            </diV>
          </div>
        </div>

      </div>
      <section class="main-conent" id="pricing" style="margin-left: 1rem;">
        
       

          <div class="container mb-4 main-container">
            <div class="row" style="margin-right: 1rem; margin-left: 1rem;">

              <div class="table-responsive">
                <table class="table table-hover mb-0">
                  <thead>
                    <tr>
                      <th>S.no</th>
                      <th>Incident ID</th>
                      <th>Project Name</th>
                      <th>Dependent Project</th>
                      <th>Description</th>
                      <th>Status</th>
                    </tr>
                  </thead>
                  <tbody>
                    <% for(var counter=0; counter< incarray.length; counter++) { %>
                      <tr>
                        <td><%= counter+1 %></td>
                      <td><a class="navi-link" href="Ticket ID" data-toggle="modal"><%= incarray[counter].Inc_Number %></a></td>
                      <td><span><%= incarray[counter].Business_Service %></span></td>
                      <td><span>ELC<br>ASD</span></td>
                      <td><span>P<%= incarray[counter].Priority %></span></td>
                      <% if (incarray[counter].Priority == 1){ %>
                      <td><span class="badge badge-danger m-0">Critical</span></td>
  
                      <% } else { %>
                        <td><span class="badge badge-warning m-0">Warning</span></td>
                        <% }%>
                        </tr>
                        <% } %>
                  </tbody>
                </table>
                <div class="m-4">
                  <nav>
                    <ul class="pagination justify-content-end">
                      <li class="page-item"><a href="tickets.html" class="page-link">Previous</a></li>
                      <li class="page-item"><a href="page2.html" class="page-link">1</a></li>
                      <li class="page-item"><a href="#" class="page-link">2</a></li>
                      <li class="page-item"><a href="#" class="page-link">3</a></li>
                      <li class="page-item"><a href="#" class="page-link">4</a></li>
                      <li class="page-item"><a href="#" class="page-link">5</a></li>
                      <li class="page-item"><a href="#" class="page-link">Next</a></li>
                    </ul>
                  </nav>
                </div>
              </div>
            </div>
          </div>
          
      </section>
    </div>




  </div>


